#ifndef HASHTABLE_H_
#define HASHTABLE_H_

struct HashTableStruct {
	int depth;
	int key;
	int check;
	int val;
};

extern HashTableStruct *HashTable;

void InitHashTable ( void );
void ClearHashTable ( void );
void DelHashTable ( void );
void InsertHashTable ( void );
int QueryHashTable ( void );

#endif /* HASHTABLE_H_ */

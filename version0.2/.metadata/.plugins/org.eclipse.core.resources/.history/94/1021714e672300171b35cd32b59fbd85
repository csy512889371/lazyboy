#ifndef HASHTABLE_H_
#define HASHTABLE_H_

struct HashTableStruct {
	int depth; // 深度
	int zorb1; // zobrist值1
	int zorb2; // zobrist值2
	int val;   // 分值
};

void InitHashTable ( void );
void ClearHashTable ( void );
void InsertHashTable ( const int depth, const int val );
int QueryHashTable ( const int depth );

#endif /* HASHTABLE_H_ */

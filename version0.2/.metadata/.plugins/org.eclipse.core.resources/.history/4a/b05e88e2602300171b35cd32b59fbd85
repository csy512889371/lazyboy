#include "hashtable.h"
#include "base.h"

HashTableStruct *HashTable;
int HashTableNum;

void InitHashTable ( int num ) {
	HashTableNum = num;
	HashTable = ( HashTableStruct * ) malloc ( HashTableNum * sizeof(HashTableStruct) );
}

void DelHashTable ( void ) {
	if ( HashTable ) {
		delete [] HashTable;
	}
}

